      <data key="d4" xml:space="preserve"><![CDATA[Zentrum für Informationsgewinnung und -analyse]]></data>
      <data key="d5" xml:space="preserve"><![CDATA[INTCEN]]></data>
      <data key="d6" xml:space="preserve"/>
